{
  "name": "ai-post-idea-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "pot": "wp i18n make-pot . languages/ai-post-idea-generator.pot --domain=ai-post-idea-generator --package-name=ai-post-idea-generator --ignore-domain",
    "phpcs": "vendor/bin/phpcs --standard=WordPress . --ignore=node_modules,vendor,src,assets",
    "phpcbf": "vendor/bin/phpcbf --standard=WordPress . --ignore=node_modules,vendor,src,assets"
  },
  "author": "Mohit Aneja <catchme@mohitaneja.com>",
  "license": "ISC",
  "devDependencies": {
    "laravel-mix": "^6.0.49"
  },
  "dependencies": {
    "alpinejs": "^3.14.5"
  }
}
